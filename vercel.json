{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/server.js"
    },
    {
      "src": "/student-login",
      "dest": "/server.js"
    },
    {
      "src": "/student-results",
      "dest": "/server.js"
    },
    {
      "src": "/parent-login",
      "dest": "/server.js"
    },
    {
      "src": "/parent-dashboard",
      "dest": "/server.js"
    },
    {
      "src": "/subject-selection",
      "dest": "/server.js"
    },
    {
      "src": "/subject-results",
      "dest": "/server.js"
    },
    {
      "src": "/test-subject-selection",
      "dest": "/server.js"
    },
    {
      "src": "/register",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
